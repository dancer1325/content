<?xml-stylesheet type="text/css" href="forSVGDOM.css"?> <!-- Passing style -->
<svg width="200" height="200" xmlns="http://www.w3.org/2000/svg">
  <!-- Define styles using the <style> element -->
  <style>
    /* CSS styles to be applied to SVG elements */
    circle {
    fill: blue;
    stroke: black;
    }
  </style>

  <!-- Draw a circle with the defined styles -->
  <circle cx="100" cy="100" r="50"/>
</svg>
